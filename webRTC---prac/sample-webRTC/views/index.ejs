<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Video Chat App</title>
    <link href="./style.css" rel="stylesheet" />
    <link
      rel="shortcut icon"
      href="data:image/jpeg;base64,"
      type="image/x-icon"
    />
  </head>
  <body>
    <div id="video-chat-form">
      <h2 class="text">Video Chat App</h2>
      <input
        id="roomName"
        type="text"
        placeholder="Room Name"
        name="roomName"
      />
      <button id="join">Join</button>
    </div>
    <div id="video-chat-rooms">
      <video id="user-video" autoplay muted></video>
      <!-- Add an audio element for playing the audio stream -->
      <audio id="user-audio" autoplay></audio>

      <video id="peer-video" autoplay></video>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.7.5/socket.io.min.js"></script>
    <script src="./script.js"></script>
  </body>
</html>
